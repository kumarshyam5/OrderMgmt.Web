        @Html.DisplayName("CustomerName")  @Html.Raw(":") @Html.DisplayFor(modelItem => item.CustomerName)
        @Html.Raw("\n")
        @Html.DisplayName("Services")  @Html.Raw(":") @Html.DisplayFor(modelItem => item.ServiceTypes)
        @Html.Raw("\n")
        @Html.Raw("\n")
    }

</textarea>


     




